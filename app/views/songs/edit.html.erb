<h1>Edit a Song</h1>
<%= form_for(@song) do |f|%>
<label>Titile</label>
<%= f.text_field :title%><br>
<label>Released </label>
<%= f.check_box  :released%><br>
<label>Release year</label>
<%= f.text_field  :release_year%><br>
<label>Artist name</label>
<%= f.text_field   :artist_name%><br>
<label>Genre</label>
<%= f.text_field :genre %><br>
<%=f.submit %>
<% end %>